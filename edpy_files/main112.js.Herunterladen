	==================================================*/

	return {

		className: className,

		autocompleteSnippets: autocompleteSnippets,

		pythonFunctions: "abs|len|ord|chr|range",

		edClassLineHelp: edClassLineHelp,

		pythonLineHelp: pythonLineHelp,

		operatorLineHelp: operatorLineHelp,

		lineHelpSyntaxError: "<span class='error'>Check this line for syntax errors.</span>",

	};

});
define('app/views/codeeditor',[

	"jquery",
	"underscore",
	"backbone",
	"utils",
	"values",
	"ui",
	"edpy",
	"edpy_data",
	//"ace/ace",

], function($, _, Backbone, Utils, Values, UI, EdPy, EdPyData/*, ace*/) {

	"use strict";

	var View = Backbone.View.extend({

		editor: null,

		snippets: null,

		el: [
			"<div id='codeEditor'>",
			"<div id='tabBar'></div>",
			"<textarea id='codeEditorTextArea'></textarea>",
			"</div>",
		].join(""),

		events: {
			"click .tab": "onClickTab",
		},

		onClickTab: function(e) {

			// Variables.

			var target = $(e.target);
			var tabID = $(e.currentTarget).data("id");
			var isCloseButton = target.hasClass("fa");

			// Focus the tab.

			EdPy.openOpenProgram(tabID);

			// Close the tab?

			if (isCloseButton) {
				if (EdPy.isDirty()) {
					var that = this;
					var message = "You have unsaved changes to this program. Are you sure you want to close it?";
					if (Values.isChromeApp) {
						UI.newConfirm({
							autoDestroy: true,
							message: message,
							onButton: function(i) {
								if (i) {
									EdPy.closeProgram(tabID);
								}
							}
						});
					} else {
						if (confirm(message)) {
							EdPy.closeProgram(tabID);
						}
					}
				} else {
					EdPy.closeProgram(tabID);
				}
			}

		},

		init: function() {

			// That.

			var that = this;

			// Create editor.

			this.editor = ace.edit("codeEditorTextArea");

			this.editor.$blockScrolling = Infinity;

			this.editor.setTheme("ace/theme/chrome");
			this.editor.getSession().setMode("ace/mode/brython");

			// Load hash handler.

			var HashHandler = ace.require("ace/keyboard/hash_handler").HashHandler;

			// Load language tools.

			ace.config.loadModule("ace/ext/language_tools", function(e) {

				//console.log(e);

				// Tools.

				var lang = ace.require("ace/lib/lang");
				var snippetManager = ace.require("ace/snippets").snippetManager;

				// Custom snippet completion functions.

				e.snippetCompleter.getCompletions = function(editor, session, pos, prefix, callback) {
					var snippetMap = snippetManager.snippetMap;
					var completions = [];
					snippetManager.getActiveScopes(editor).forEach(function(scope) {
						var snippets = snippetMap[scope] || [];
						for (var i=snippets.length; i--;) {
							var s = snippets[i];
							//console.log(s);
							//console.log(s.name);
							//console.log(s.tabTrigger);
							var caption = s.name || s.tabTrigger;
							if (!caption) {
								continue;
							} else {
								//console.log(caption);
								//console.log(" ");
								completions.push({
									caption: caption,
									help: s.help,
									fullHelp: s.fullHelp,
									snippet: s.content,
									title: s.title,
									meta: "Edison",
									type: "snippet",
								});
							}
						}
					}, this);
					callback(null, completions);
				};

				e.snippetCompleter.getDocTooltip = function(item) {
					if (item.type == "snippet" && !item.docHTML) {
						var fullHelp = item.fullHelp ? "<br><p data-title='" + lang.escapeHTML(item.caption) + "' class='btn btn-xs btn-success btn-view-documentation ten-margin-top'>View Documentation</p>" : "";
						item.docHTML = [
							"<b>{0}</b>".format(lang.escapeHTML(item.title)),
							"<hr>",
							"{0}{1}".format(lang.escapeHTML(item.help), fullHelp),
						].join("");
					}
				};

				// Set completers.

				e.setCompleters([e.snippetCompleter]);

				// Set editor options.

				that.editor.setOptions({
					enableLiveAutocompletion: true,
					enableBasicAutocompletion: true,
					enableSnippets: true
				});

				// Register snippets.

				ace.config.loadModule("ace/snippets/brython", function(m) {

					if (m) {

						// Register snippets.

						snippetManager.register(EdPyData.autocompleteSnippets, m.scope);

						// Reference to snippets.

						that.snippets = m.snippets;

						// Snippets registered event.

						that.trigger("event", {
							type: "snippetsRegistered",
						});

					}

				});

			});

			// Event listeners.

			this.editor.container.addEventListener("keydown", function(e) {
				setTimeout(function() {
					that.changeSelection("onKeyDown()");
				}, 50);
			}, true);

			this.editor.on("change", function(e) {

				// Change text event.

				if (that.editor.curOp && that.editor.curOp.command.name) {
					EdPy.hideAllCompilerOutput();
					EdPy.setProgramText(that.editor.getValue());
					EdPy.isDirty(true);
					that.trigger("event", {
						type: "changeText",
						text: that.editor.getValue(),
					});
				}

				// Change event.

				that.changeSelection("onChange()");

			});

			this.editor.selection.on("changeSelection", function(e) {
				that.changeSelection("onChangeSelection()");
			});

			// Render view.

			this.render();

		},

		changeSelection: function(source, keyboardEvent) {

			// That.

			var that = this;

			// Get range and modify contents slightly.

			var range = that.editor.getSelectionRange();

			// Use the whole line if they haven't specified a selection.

			if (range.end.column == range.start.column) {
				range.start.column = 0;
				range.end.column = 100000;
			}

			// Trigger event.

			this.trigger("event", {
				type: "changeSelection",
				startLine: range.start.row,
				endLine: range.end.row,
				text: that.editor.session.getTextRange(range),
			});

			// Log.

			/*if (Values.logEnabled) {
			console.log("Change selection:");
			console.log("Source:", source);
			console.log("Line count:", this.editor.session.doc.getAllLines().length);
			console.log("Range:", range);
			console.log(" ");
		}*/

	},

	highlightErrors: function() {
		var that = this;
		var Range = ace.require("ace/range").Range;
		this.removeHighlights();
		EdPy.compilerOutput.each(function(item, index, all) {
			var lineNumber = item.get("line")-1;
			if (item.get("visible") && !item.get("deleted")) {
				item.set("markerID", that.editor.session.addMarker(new Range(lineNumber, 1000, lineNumber, 0), "ace_active-line ace_error-line", "fullLine"));
			}
		});
		this.editor.focus();
	},

	removeHighlights: function() {
		var that = this;
		EdPy.compilerOutput.each(function(item, index, all) {
			that.editor.session.removeMarker(item.get("markerID"));
		});
		this.editor.focus();
	},

	goToLine: function(e) {
		this.editor.selection.moveTo(e.line-1, e.column);
		this.editor.focus();
	},

	render: function() {

		// Loop the open programs and create tabs for each of them.

		var tabs = [];

		EdPy.programData.open.each(function(item, index, all) {
			var program = EdPy.getProgram(item.get("id"));
			var focusedClass = program.get("id") == EdPy.getCurrentProgramID() ? " tab-focused" : "";
			tabs.push("<div data-id='{2}' class='tab{0}'><p>{1}</p><i class='fa fa-times'></i></div>".format(focusedClass, program.get("name"), program.get("id")));
		});

		this.$el.find("#tabBar").html(tabs.join(""));

		// Update the editor to use the correct program text.

		this.editor.setValue(EdPy.getProgram().get("text"));

		// Position selection.

		var row = this.editor.session.getLength() - 1;
		var column = this.editor.session.getLine(row).length;

		this.editor.selection.moveTo(row, column);

		// Focus the editor.

		this.editor.focus();

	},

	initialize: function() {

		// Set HTML.

		this.$el.html(this.html);

		// Listen for events on the EdPy object.

		EdPy.on("change", this.render, this);
		EdPy.on("output.update", this.highlightErrors, this);
		EdPy.on("output.reset", this.removeHighlights, this);
		EdPy.on("output.select", this.goToLine, this);

	},

});

return View;

});
define('app/views/compileroutput',[
	"jquery",
	"underscore",
	"backbone",
	"edpy",
	"values",
	"transition",
], function($, _, Backbone, EdPy, Values, Utils, transition) {

	"use strict";

	var defaultMessageHTML = "<p>There are no errors in your code.</p>";

	var View = Backbone.View.extend({

		id: "compilerOutput",

		events: {
			"click .delete-row" : "onDeleteOutput",
			"click .toggle-row" : "onToggleOutput",
			"click .go-to-line" : "onGoToLine",
		},

		html: [
			"<div class='resize-vertical float-top'></div>",
			"<div class='title-bar popup-title-bar no-shadow clearfix'>",
			"<ul class='title-bar-left'><li class='title'><a title='Use the \"Check Code\" button in the top right corner to find errors in your code. If any errors are found in your code they will be listed in this window.'><i class='fa fa-info-circle'></i> Compiler Output</a></li></ul>",
			"</div>",
			"<div id='contentWrapper'>",
			defaultMessageHTML,
			"</div>",
		].join(""),

		onDeleteOutput: function(e) {
			EdPy.deleteCompilerOutput($(e.currentTarget).parent().data().id);
		},

		onToggleOutput: function(e) {
			EdPy.toggleCompilerOutput($(e.currentTarget).parent().data().id);
		},

		onGoToLine: function(e) {
			EdPy.goToLine($(e.currentTarget).parent().data().id);
		},

		render: function() {

			// Variables.

			var newHTML = "";

			// Create error messages?

			if (EdPy.compilerOutput.length) {
				EdPy.compilerOutput.each(function(item, index, all) {
					if (!item.get("deleted")) {
						newHTML += [
							"<div class='output-row' data-id='{0}'>".format(item.get("id")),
							//"<a class='output-button delete-row'><i class='fa fa-trash'></i></a>",
							"<a class='output-button toggle-row'><i class='fa fa-{0}'></i></a>".format(item.get("visible") ? "eye-slash" : "eye"),
							"<a class='output-button go-to-line'><i class='fa fa-dot-circle-o'></i></a>",
							"<span class='output-message'><strong>{0}, {1}:</strong> {2}</div>".format(item.get("line"), item.get("column"), item.get("message")),
							"</div>",
						].join("");
					}
				});
			}

			if (!newHTML) {
				this.$el.find("#contentWrapper").html(defaultMessageHTML);
			} else {
				this.$el.find("#contentWrapper").html(newHTML);
			}

		},

		flashBackground: function() {

			// Variables.

			var $background = this.$el.find("#contentWrapper");

			// Flash the background so the user knows something has happened.

			$background.transition({
				"background-color": Values.highlightColour,
			}, 250, "ease").transition({
				"background-color": "transparent",
			}, 250, "ease").transition({
				"background-color": Values.highlightColour,
			}, 250, "ease").transition({
				"background-color": "transparent",
			}, 250, "ease").transition({
				"background-color": Values.highlightColour,
			}, 250, "ease").transition({
				"background-color": "transparent",
			}, 250, "ease").transition({
				"background-color": Values.highlightColour,
			}, 250, "ease").transition({
				"background-color": "transparent",
			}, 250, "ease");

		},

		initialize: function() {

			// Set HTML.

			this.$el.html(this.html);

			// Listen for events on the EdPy object.

			EdPy.on("output.update", this.render, this);
			EdPy.on("output.flash", this.flashBackground, this);

		},

	});

	return View;

});
define('app/views/helpdocumentation',[
	"jquery",
	"underscore",
	"backbone",
	"values",
	"utils",
	"edpy",
	"transition",
], function($, _, Backbone, Values, Utils, EdPy, transition) {

	// Strict!

	"use strict";

	// Create view.

	var View = Backbone.View.extend({

		listData: [],
		listRows: [],

		id: "helpDocumentation",

		events: {
			"keyup #searchField": "onKeyUpSearchField",
			"click .open-documentation": "onOpenDocumentation",
			"click .close-documentation": "onCloseDocumentation",
		},

		html: {
			helpFooter: "<hr><button class='close-documentation btn btn-block'><i class='fa fa-arrow-left'></i> Back to List</button>",
			helpWrapper: [
				"<div class='title-bar popup-title-bar no-shadow clearfix'>",
				"<ul class='title-bar-left'><li class='close-button hidden'><a id='closeDocumentation' class='close-documentation' href='#'><i class='fa fa-times'></i></a></li><li class='title'><a>Documentation</a></li></ul>",
				"</div>",
				"<div id='helpWrapper'></div>",
			].join(""),
			listAndItemWrapper: [
				"<div id='listWrapper'>",
				"<input id='searchField' type='text' placeholder='Search documentation...'>",
				"<div id='listItemsWrapper'></div>",
				"</div>",
				"<div id='documentationWrapper' class='hidden'>",
				"<div id='contentWrapper'></div>",
				"</div>",
			].join(""),
			noResults: [
				"<div class='message-background documentation'>",
				"<div class='message-outer'>",
				"<div class='message-inner'>",
				"<div class='spinner-wrap'>",
				"<p>There is no documentation at this time.</p>",
				"</div>",
				"</div>",
				"</div>",
				"</div>",
			].join(""),
			loading: [
				"<div class='message-background documentation'>",
				"<div class='message-outer'>",
				"<div class='message-inner'>",
				"<div class='spinner-wrap'>",
				"<div class='sprite-spinner'>",
				"<img src='img/loading.png' alt=''/>",
				"</div>",
				"<p>Loading Documentation</p>",
				"</div>",
				"</div>",
				"</div>",
				"</div>",
			].join(""),
		},

		onKeyUpSearchField: function(e) {

			// Variables.

			var rows = this.$el.find("#listWrapper li").hide();
			var keywords = $(e.target).val();

			// Have they entered keywords? Search the rows. If not, show all the rows.

			if (keywords.length) {
				var data = keywords.split(";");
				if (typeof data == "string") {
					rows.filter(function() {
						var $t = $(this);
						var keyword = data.trim().toLowerCase();
						if ($t.text().toLowerCase().indexOf(keyword) > -1) {
							return true;
						}
						return false;
					}).show();
				} else {
					rows.filter(function() {
						var $t = $(this);
						for (var d=0; d<data.length; d++) {
							var keyword = data[d].trim().toLowerCase();
							if (keyword) {
								if ($t.text().toLowerCase().indexOf(keyword) > -1) {
									return true;
								}
							}
						}
						return false;
					}).show();
				}
			} else {
				rows.show();
			}

		},

		onCloseDocumentation: function(e) {

			// Hide / show buttons.

			this.$el.find(".title-bar .title").removeClass("hidden");
			this.$el.find(".title-bar .close-button").addClass("hidden");

			// Hide documentation.

			this.$el.find("#documentationWrapper").addClass("hidden");

			// Show search results.

			this.$el.find("#listWrapper").removeClass("hidden");

			// Close documentation event.

			this.trigger("event", {
				type: "closeDocumentation",
			});

		},

		onOpenDocumentation: function(e) {

			// Variables.

			var index = $(e.target).data("index");

			// Hide search results.

			this.$el.find("#listWrapper").addClass("hidden");

			// Update title.

			this.$el.find("#closeDocumentation").html("<i class='fa fa-times'></i>&nbsp;&nbsp;" + this.listData[index].title);

			// Hide / show buttons.

			this.$el.find(".title-bar .title").addClass("hidden");
			this.$el.find(".title-bar .close-button").removeClass("hidden");

			// Update the content wrapper's HTML.

			this.$el.find("#contentWrapper").html(this.listData[index].html + this.html.helpFooter);

			// If this is the chrome app, iterate the images and download them.

			if (Values.isChromeApp) {
				var images = this.$el.find("img");
				$.each(images, function(i, image) {
					var $image = $(image);
					var xhr = new XMLHttpRequest();
					xhr.open("GET", $image.attr("src"), true);
					xhr.responseType = "blob";
					xhr.onload = function(e) {
						$image.attr("src", window.URL.createObjectURL(this.response));
					};
					xhr.send();
				});
			}

			// Show documentation.

			this.$el.find("#documentationWrapper").removeClass("hidden");

			// Open documentation event.

			this.trigger("event", {
				type: "openDocumentation",
			});

		},

		openItem: function(title) {

			// Remove parentheses from title?

			if (title.substring(title.length - 2, title.length) == "()") {
				title = title.slice(0, -2);
			}

			// Find the row that matches the title.

			var rows = this.$el.find("#listWrapper li").filter(function() {
				var $t = $(this);
				var keyword = title.trim().toLowerCase();
				if ($t.text().toLowerCase().indexOf(keyword) > -1) {
					return true;
				}
				return false;
			});

			// Open the item.

			if (rows.length) {

				// Click the a tag within the row.

				var item = $(rows[0]).find("a").trigger("click");

				// Flash the element so the user knows something has happened.

				var $background = this.$el.find("#helpWrapper");

				$background.transition({
					"background-color": Values.highlightColour,
				}, 250, "ease").transition({
					"background-color": "transparent",
				}, 250, "ease").transition({
					"background-color": Values.highlightColour,
				}, 250, "ease").transition({
					"background-color": "transparent",
				}, 250, "ease").transition({
					"background-color": Values.highlightColour,
				}, 250, "ease").transition({
					"background-color": "transparent",
				}, 250, "ease").transition({
					"background-color": Values.highlightColour,
				}, 250, "ease").transition({
					"background-color": "transparent",
				}, 250, "ease");

			}

		},

		getList: function() {

			// That.

			var that = this;

			// Reset list data.

			this.listData = [];
			this.listRows = [];

			// Request the help documentation from the server.

			EdPy.api.read("help", {app: Values.app}, function(response, status, xhr) {
				/*response.success = true;
				response.results = [
				{
				title: "Ed.PlayTune()",
				keywords: "music,sound,tune",
				html: "<p>This is a description for item one.</p>",
			},
			{
			title: "Ed.Drive()",
			keywords: "drive,motor,speed",
			html: "<p>This is a description for item two.</p>",
		},
	];*/
	if (response.success) {
		// Iterate the results and create the list.
		$.each(response.results, function(i, data) {
			// Store the results.
			that.listData.push(data);
			// Create a row for this help item.
			that.listRows.push("<li><a class='open-documentation' data-index='{0}' href='#'>{1} <span class='hidden'>{2}</span></a></li>".format(i, data.title, data.keywords));
		});
		// Update the list wrapper's HTML.
		that.$el.find("#helpWrapper").html(that.html.listAndItemWrapper).find("#listItemsWrapper").html([
			"<ul>",
			that.listRows.join(""),
			"</ul>"
		].join(""));
		// Fire event.
		that.trigger("event", {
			type: "openDocumentation",
		});
	} else {
		that.$el.find("#helpWrapper").html(that.html.noResults);
	}
});

},

initialize: function() {

	// Set this view's HTML.

	this.$el.html(this.html.helpWrapper).find("#helpWrapper").html(this.html.loading);

	// Init the loading spinner.

	this.$el.find(".sprite-spinner").each(function(i) {
		new SpriteSpinner(this, {interval: 50}).start();
	});

	// Get table of contents.

	this.getList();

},

});

// Return view.

return View;

});
define('app/views/linehelp',[
	"jquery",
	"underscore",
	"backbone",
	"utils",
	"values",
	"edpy_data"
], function($, _, Backbone, Utils, Values, EdPyData) {

	// Strict!

	"use strict";

	// Create view.

	var View = Backbone.View.extend({

		// Properties.

		id: "lineHelp",

		html: [
			"<div class='resize-vertical float-top'></div>",
			"<div class='title-bar popup-title-bar no-shadow clearfix'>",
			"<ul class='title-bar-left'><li class='title'><a title='Highlight a line of code in the text editor to get an explanation of what that code does.'><i class='fa fa-info-circle'></i> Line Help</a></li></ul>",
			"</div>",
			"<div id='textWrapper'>",
			"<p>No help text for this line.</p>",
			"</div>",
		].join(""),

		setOptionsTimerID: 0,
		setOptionsTimerDelay: 500,

		distanceUnits: "Ed.CM",
		tempoSpeed:  EdPyData.edClassLineHelp.constants["Ed.TEMPO_MEDIUM"],

		limits: {
			assignment: 2,
			import: 2,
			return: 2,
			pass: 1,
			break: 1,
			continue: 1,
			userFunction: 2,
			conditional: 2,
			while: 2,
			loop: 4,
		},

		text: "",
		results: [],
		resultsBroken: [],
		resultsWithoutComments: [],

		// Functions / methods.

		getText: function() {
			return this.$el.find("p").text();
		},

		setText: function(newText) {
			this.$el.find("p").html(newText ? newText : "No help text for this line.");
		},

		replaceKey: function(key) {
			if (EdPyData.edClassLineHelp.constants.hasOwnProperty(key)) {
				return EdPyData.edClassLineHelp.constants[key];
			}
			return key;
		},

		formatFunction: function(name, params) {

			// Variables.

			var directionName = params[0];
			var speedName = params[1];
			var distanceName = params[2];

			var param1 = this.replaceKey(directionName);
			var param2 = this.replaceKey(speedName);
			var param3 = this.replaceKey(distanceName);

			var functionsList = EdPyData.edClassLineHelp.functions;

			// Does this function exist in the list?

			if (functionsList.hasOwnProperty(name)) {

				var functionHelpText = functionsList[name];

				if (name == "Ed.Drive" || name == "Ed.DriveLeftMotor" || name == "Ed.DriveRightMotor") {

					if (directionName == "Ed.SPIN_RIGHT" || directionName == "Ed.SPIN_LEFT") {

						return functionHelpText.format(param1, param2, param3, "degrees");

					} else {

						var distance = parseInt(param3);
						var distanceUnits = EdPyData.edClassLineHelp.constants[this.distanceUnits].plural;

						if (distanceName == "Ed.DISTANCE_UNLIMITED") {
							distance = EdPyData.edClassLineHelp.constants[params[2]];
							distanceUnits = "";
						} else if (directionName == "Ed.FORWARD_LEFT" || directionName == "Ed.FORWARD_RIGHT" || directionName == "Ed.BACKWARD_LEFT" || directionName == "Ed.BACKWARD_RIGHT") {
							distanceUnits = "degrees";
						} else {
							if (isNaN(distance)) {
								distance = "<span class='error'>INVALID ARGUMENT</span>";
							} else {
								if (distance == 1) {
									distanceUnits = EdPyData.edClassLineHelp.constants[this.distanceUnits].singular;
								}
							}
						}

						return functionHelpText.format(param1, param2, distance, distanceUnits);

					}

				} else if (name == "Ed.PlayTone") {

					return functionHelpText.format(param1, param2, this.tempoSpeed);

				} else if (name == "Ed.PlayTune") {

					return functionHelpText.format(param1, this.tempoSpeed);

				} else {

					return functionHelpText.format(param1, param2);

				}

			}

			return null;

		},

		isEdFunction: function(value) {
			if (EdPyData.edClassLineHelp.functions.hasOwnProperty(value)) {
				return EdPyData.edClassLineHelp.functions[value];
			}
			return false;
		},

		isEdConstant: function(value) {
			var constantsList = EdPyData.edClassLineHelp.constants;
			if (constantsList.hasOwnProperty(value)) {
				if (typeof constantsList[value] == "object") {
					return constantsList[value].singular;
				}
				return constantsList[value];
			}
			return false;
		},

		overLimit: function(type) {
			return this.resultsWithoutComments.length > this.limits[type];
		},

		withinLimit: function(type) {
			//console.log(type, this.resultsWithoutComments.length, this.limits[type]);
			return this.resultsWithoutComments.length == this.limits[type];
		},

		parseAsAssignment: function() {

			// Type.

			var type = "assignment";

			var operator = "=";

			var assigneeAndAssignment = "";

			// Remove whitespace from text.

			/*this.resultsBroken = this.text.split(" ");

			//

			// Check if it has comments.

			this.resultsWithoutComments = this.resultsBroken;

			var index = Utils.findStringInArray(this.resultsBroken, /#.*?$/);

			if (index != -1) {
				this.resultsWithoutComments = this.resultsBroken.slice(0, index);
			}*/

			// Find the assignee and assignment.

			if (this.resultsWithoutComments.length == 1) {
				assigneeAndAssignment = this.resultsWithoutComments[0];
			}

			if (this.resultsWithoutComments.length == 2) {
				assigneeAndAssignment = this.resultsWithoutComments[0] + this.resultsWithoutComments[1];
			}

			if (this.resultsWithoutComments.length == 3) {
				assigneeAndAssignment = this.resultsWithoutComments[0] + this.resultsWithoutComments[1] + this.resultsWithoutComments[2];
			}

			// Split the string?

			if (assigneeAndAssignment) {
				this.resultsWithoutComments = assigneeAndAssignment.split(operator);
			}

			/*console.log(" ");
			//console.log(index);
			console.log(assigneeAndAssignment);
			console.log(this.resultsBroken);
			console.log(this.resultsWithoutComments);*/

			// If the results are the correct length create the line help text.

			if (this.overLimit(type)) {

				// We don't support compound assignments at this stage so return "null" which will result in a "No help text for this line." message.

				return null;

			} else {

				if (this.withinLimit(type)) {

					// Variables.

					var assignee = this.resultsWithoutComments[0];
					var assignment = this.resultsWithoutComments[1];

					//var operator = this.resultsBroken[1];

					/*console.log(assignee);
					console.log(operator);
					console.log(assignment);*/

					// Check if assignee is part of the Ed class.

					if (this.parseAsEdClassFunction(assignee)) {
						assignee = this.parseAsEdClassFunction(assignee).text;
					}

					// Check if assignment is part of the Ed class.

					if (this.parseAsEdClassFunction(assignment)) {
						assignment = this.parseAsEdClassFunction(assignment).text;
					}

					if (this.isEdConstant(assignment)) {
						assignment = this.isEdConstant(assignment);
					}

					// Check if the operator exists. If it does, return the help text.

					if (EdPyData.operatorLineHelp.hasOwnProperty(operator)) {
						return {
							success: true,
							text: EdPyData.pythonLineHelp.operator.format(assignee, EdPyData.operatorLineHelp[operator], assignment),
						};
					}

				}

			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsComment: function() {

			return {
				success: true,
				text: EdPyData.pythonLineHelp.comment.format(this.text),
			};

		},

		parseAsImport: function() {

			if (this.withinLimit("import")) {
				return {
					success: true,
					text: EdPyData.pythonLineHelp.import.format(this.resultsBroken[1]),
				};
			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsReturn: function() {

			if (this.withinLimit("return")) {

				var returnValue = this.resultsBroken[1];

				if (this.parseAsEdClassFunction(returnValue)) {
					returnValue = this.parseAsEdClassFunction(returnValue).text;
				}

				return {
					success: true,
					text: EdPyData.pythonLineHelp.return.format(returnValue),
				};

			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsPass: function() {

			if (this.withinLimit("pass")) {
				return {
					success: true,
					text: EdPyData.pythonLineHelp.pass,
				};
			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsBreak: function() {

			if (this.withinLimit("break")) {
				return {
					success: true,
					text: EdPyData.pythonLineHelp.break,
				};
			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsContinue: function() {

			if (this.withinLimit("continue")) {
				return {
					success: true,
					text: EdPyData.pythonLineHelp.continue,
				};
			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsUserFunction: function() {

			// Remove params from name.

			var params = /\(([^)]+)\)/.exec(this.results[0]) || ["()"];

			this.text = Utils.string.remove(this.text, params[1]);
			this.resultsBroken = this.text.split(" ");

			// Create the function name.

			var functionNameWithColon = Utils.string.replace(this.resultsBroken[1], "()", params[0]);
			var functionNameWithoutColon = Utils.string.removeLast(functionNameWithColon, ":");

			// If the results are the correct length create the line help text.

			if (this.withinLimit("userFunction")) {
				return {
					success: true,
					text: EdPyData.pythonLineHelp.userFunction.format(functionNameWithoutColon),
				};
			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsConditional: function() {

			// Variables.

			var type = "conditional";

			var operator = "";

			var condition1AndCondition2 = "";

			// Remove the colon if it exists.

			this.text = Utils.string.removeLast(this.text, ":");
			this.resultsBroken = this.text.split(" ");

			// Check if it has comments.

			this.resultsWithoutComments = this.resultsBroken;

			var index = Utils.findStringInArray(this.resultsBroken, /#.*?$/);

			if (index != -1) {
				this.resultsWithoutComments = this.resultsBroken.slice(0, index);
			}

			// Find the conditions.

			if (this.resultsWithoutComments.length == 2) {
				condition1AndCondition2 = this.resultsWithoutComments[1];
			}

			if (this.resultsWithoutComments.length == 3) {
				condition1AndCondition2 = this.resultsWithoutComments[1] + this.resultsWithoutComments[2];
			}

			if (this.resultsWithoutComments.length == 4) {
				condition1AndCondition2 = this.resultsWithoutComments[1] + this.resultsWithoutComments[2] + this.resultsWithoutComments[3];
			}

			// Find the operator.

			var operators = ["==", "!=", ">", "<", ">=", "<="]

			for (var i=0; i<operators.length; i++) {

				if (condition1AndCondition2.indexOf(operators[i]) != -1) {

					// Store the operator.

					operator = operators[i];

					// Update the results.

					this.resultsWithoutComments = condition1AndCondition2.split(operator);

					// Bail out!

					break;

				}

			}

			/*console.log(operator);
			console.log(condition1AndCondition2);
			console.log(this.resultsBroken);
			console.log(this.resultsWithoutComments);*/

			// If the results are the correct length create the line help text.

			if (this.overLimit(type)) {

				// We don't support compound conditionals at this stage so return null which will result in a "No help text for this line." message.

				return null;

			} else {

				if (this.withinLimit(type)) {

					//console.log("WOOOOTTTt");

					// Variables.

					var first = this.resultsWithoutComments[0];
					var last = this.resultsWithoutComments[1];

					// Check if 'first' is part of the Ed class.

					if (this.parseAsEdClassFunction(first)) {
						first = this.parseAsEdClassFunction(first).text;
					}

					// Check if 'last' is part of the Ed class.

					if (this.parseAsEdClassFunction(last)) {
						last = this.parseAsEdClassFunction(last).text;
					}

					// Modify the strings a little bit.

					first = Utils.string.removeFirst(first, "(");
					last = Utils.string.removeLast(last, ")");

					// Check if the operator exists. If it does, return the help text.

					if (EdPyData.operatorLineHelp.hasOwnProperty(operator)) {
						return {
							success: true,
							text: EdPyData.pythonLineHelp.conditional.format(first, EdPyData.operatorLineHelp[operator], last),
						};
					}

				}

			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsWhile: function() {

			return {
				success: true,
				text: EdPyData.pythonLineHelp.while,
			};

			/*
			// Variables.

			var type = "while";

			// Check if the results are over the limit / within the limit.

			if (this.overLimit(type)) {
				return null;
			} else {
				if (this.withinLimit(type)) {
					return {
						success: true,
						text: EdPyData.pythonLineHelp.while,
					};
				}
			}

			// Return error.

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};
			*/

		},

		parseAsLoop: function() {

			// First part of loop.

			var first = this.resultsBroken[1];

			if (first.indexOf("range") != -1) {
				var params = /\(([^)]+)\)/.exec(first) || ["()", "0"];
				params = params[1].split(",");
				if (params.length > 1) {
					return null;
				}
				first = parseInt(params[0]);
			}

			// Last part of loop.

			var last = this.resultsBroken[3];

			if (last.indexOf("range") != -1) {
				var params = /\(([^)]+)\)/.exec(last) || ["()", "0"];
				params = params[1].split(",");
				if (params.length > 1) {
					return null;
				}
				last = parseInt(params[0]);
			}

			// If the results are the correct length create the line help text.

			if (this.withinLimit("loop")) {
				return {
					success: true,
					text: EdPyData.pythonLineHelp.loop.format(first, last),
				};
			}

			return {
				error: true,
				text: EdPyData.lineHelpSyntaxError,
			};

		},

		parseAsEdClassFunction: function() {

			var results;
			var regExp = /Ed\..*?\(.*?\)/ig;

			while ((results = regExp.exec(this.text)) !== null) {

				// Variables.

				var functionName = results[0];
				var params = /\(([^)]+)\)/.exec(functionName) || [];

				// If this function has params remove them from the function name.

				if (params.length) {
					functionName = functionName.replace(params[1], "");
					params = params[1].replace(/\s/g, "").split(",");
				}

				// Fill the params array with ""

				for (var i=0; i<3; i++) {

					if (params[i] && params[i].trim() == "") {
						params[i].pop();
					}

					if (!params[i]) {
						params.push("<span class='error'>MISSING ARGUMENT</span>");
					}

				}

				// Remove the parentheses from the function name.

				functionName = functionName.replace("()", "");

				// Get formatted function help text.

				return {
					success: true,
					text: this.formatFunction(functionName, params),
				};

			}

			return false;

		},

		parseAsEdClassOther: function() {
			if (EdPyData.edClassLineHelp.variables.hasOwnProperty(this.text)) {
				return {
					success: true,
					text: EdPyData.edClassLineHelp.variables[this.text],
				};
			}
			return false;
		},

		parseText: function(text) {

			// Variables.

			var parseAsList = {
				parseAsComment: { regExp: /^#/ig },
				parseAsImport: { regExp: /^import.*?\s?#?.*?$/ig },
				parseAsReturn: { regExp: /\s?return.*?\s?#?.*?$/ig },
				parseAsPass: { regExp: /\s?pass\s?#?.*?$/ig },
				parseAsBreak: { regExp: /\s?break\s?#?.*?$/ig },
				parseAsContinue: { regExp: /\s?continue\s?#?.*?$/ig },
				parseAsWhile: { regExp: /\s?while.*?:\s?#?.*?$/ig },
				parseAsLoop: { regExp: /\s?for\s.*?\sin\srange\(.*?\):\s?#?.*?$/ig },
				parseAsUserFunction: { regExp: /\s?def.*?:\s?#?.*?$/ig },
				parseAsConditional: { regExp: /\s?if.*?:\s?#?.*?$/ig },
				parseAsAssignment: { regExp: /\s?.*?=.*?/ig },
				parseAsEdClassFunction: {},
				parseAsEdClassOther: {},
			};

			// Parse text.

			for (var key in parseAsList) {

				// Variables.

				var properties = parseAsList[key];
				var regExp = properties.regExp;

				var result;
				var results;

				var paramResults = [];

				// Reset properties.

				this.text = "";
				this.results = [];
				this.resultsBroken = [];
				this.resultsWithoutComments = [];

				// Remove spaces from function parameters.

				var paramRegExp = /\(.*?\)/ig;

				while ((paramResults = paramRegExp.exec(text)) !== null) {

					var paramsWithWhitespace = paramResults[0];
					var paramsWithoutWhitespace = Utils.string.removeWhitespace(paramsWithWhitespace);

					text = Utils.string.replace(text, paramsWithWhitespace, paramsWithoutWhitespace);

				}

				// Try parsing the text using a regular expression?

				if (regExp) {

					while ((results = regExp.exec(text)) !== null) {

						// Store results.

						this.results = results;

						// Store text.

						this.text = Utils.string.removeLast(this.results.input, ":");
						this.results[0] = this.text;
						this.results.input = this.text;

						// Break results up.

						this.resultsBroken = this.results.input.split(" ");

						// Make a copy of the results minus comments.

						this.resultsWithoutComments = this.resultsBroken;

						var index = Utils.findStringInArray(this.resultsBroken, /#.*?$/);

						if (index != -1) {
							this.resultsWithoutComments = this.resultsBroken.slice(0, index);
						}

						// Get a result for this type.

						result = this[key]();

					}

				} else {

					// Store text.

					this.text = text;

					// Get a result for this type.

					result = this[key]();

				}

				// Result?

				if (result) {
					if (result.success) {
						return Utils.string.formatAsSentence(result.text);
					} else if (result.error) {
						return result.text;
					}
				}

			}

			// Return nothing just in case.

			return null;

		},

		generateText: function() {



		},

		parseLine: function(text) {

			// Variables.

			var regExpList = {
				"conditional": /\s?if.*?:\s?#?.*?$/ig,
				"assignment": /\s?.*?=.*?/ig,
				"import": /^import.*?\s?#?.*?$/ig,
				"return": /\s?return.*?\s?#?.*?$/ig,
				"pass": /\s?pass\s?#?.*?$/ig,
				"break": /\s?break\s?#?.*?$/ig,
				"continue": /\s?continue\s?#?.*?$/ig,
				"userFunction": /\s?def.*?:\s?#?.*?$/ig,
				"while": /\s?while.*?:\s?#?.*?$/ig,
				"for": /\s?for\s.*?\sin\srange\(.*?\):\s?#?.*?$/ig,
			};

			var lineInformation = {};

			// Remove unnecessary whitespace from text.

			text = Utils.string.reduceWhitespace(text);

			// Remove spaces from function parameters.

			var paramResults = [];
			var paramRegExp = /\(.*?\)/ig;

			while ((paramResults = paramRegExp.exec(text)) !== null) {
				var paramsWithWhitespace = paramResults[0];
				var paramsWithoutWhitespace = Utils.string.removeWhitespace(paramsWithWhitespace);
				text = Utils.string.replace(text, paramsWithWhitespace, paramsWithoutWhitespace);
			}

			// Parse text.

			for (var key in regExpList) {

				// Variables.

				var type = key;
				var regExp = regExpList[key];
				var results = [];

				// Find out what type of line this is.

				while ((results = regExp.exec(text)) !== null) {

					// Line information.

					lineInformation = {
						type: key,
						text: {
							original: results.input,
							trimmed: Utils.string.removeLast(results.input, ":"),
							broken: results.input.split(" "),
						},
						functions: [],
						comments: [],
					};

				}

			}

			//console.log(lineInformation);

		},

		setOptions: function(e) {

			// That.

			var that = this;

			// Cancel timer?

			if (this.setOptionsTimerID) {
				clearTimeout(this.setOptionsTimerID);
			}

			// Set options on a timer.

			this.setOptionsTimerID = setTimeout(function() {

				// Variables.

				var result;
				var results = [];
				var text = e.text.replace(/\s/g, "");

				var constantsList = EdPyData.edClassLineHelp.constants;

				// Set distance units option.

				var distanceRegExp = /Ed.DistanceUnits=Ed.CM|Ed.DistanceUnits=Ed.INCH|Ed.DistanceUnits=Ed.TIME/ig;

				while ((result = distanceRegExp.exec(text)) !== null) {
					results.push(result);
				}

				if (results.length) {
					var broken = results[results.length-1][0].split("=");
					if (broken.length && constantsList.hasOwnProperty(broken[1])) {
						that.distanceUnits = broken[1];
					} else {
						that.distanceUnits = "Ed.CM";
					}
				}

				// Reset variables.

				result = null;
				results = [];

				// Set tempo speed option.

				var tempoRegExp = /Ed.Tempo=Ed.TEMPO_VERY_SLOW|Ed.Tempo=Ed.TEMPO_SLOW|Ed.Tempo=Ed.TEMPO_MEDIUM|Ed.Tempo=Ed.TEMPO_FAST|Ed.Tempo=Ed.TEMPO_VERY_FAST/ig;

				while ((result = tempoRegExp.exec(text)) !== null) {
					results.push(result);
				}

				if (results.length) {
					var broken = results[results.length-1][0].split("=");
					if (broken.length && constantsList.hasOwnProperty(broken[1])) {
						that.tempoSpeed = constantsList[broken[1]];
					} else {
						that.tempoSpeed = constantsList["Ed.TEMPO_MEDIUM"];
					}
				}

			}, this.setOptionsTimerDelay);

		},

		parseEvent: function(e) {
			if (typeof e == "object") {
				if (e.startLine == e.endLine && e.text.trim() !== "") {
					//this.parseLine(e.text);
					this.setText(this.parseText(Utils.string.reduceWhitespace(e.text)));
				} else {
					if (e.endLine > e.startLine) {
						this.setText("Only one line can be selected.");
					} else {
						this.setText();
					}
				}
			} else {
				this.setText();
			}
		},

		initialize: function() {
			this.$el.html(this.html);
		},

	});

	// Return view.

	return View;

});
define('app/views/main',[

	"jquery",
	"underscore",
	"backbone",

	"edpy",
	"values",
	"utils",
	"ui",

	"app/views/navigationbar",
	"app/views/userprograms",
	"app/views/codeeditor",
	"app/views/compileroutput",
	"app/views/helpdocumentation",
	"app/views/linehelp",

	"app/libs/i18n",

], function($, _, Backbone, EdPy, Values, Utils, UI, NavigationBar, UserPrograms, CodeEditor, CompilerOutput, HelpDocumentation, LineHelp, i18n) {

	/*==================================================

	Strict

	==================================================*/

	"use strict";

	/*==================================================

	Private

	==================================================*/

	var a = function(message) {
		UI.newAlert({
			message: message,
		});
	};

	var updateLanguage = function(main, newLanguage, i18n_key, callback) {
		if (i18n.isDifferentLanguage(newLanguage)) {
			UI.newConfirm({
				autoDestroy: true,
				message: i18n.get(i18n_key).format(i18n.getLanguageNames(newLanguage)),
				onButton: function(i) {
					if (i === 1) {
						// Update language settings.
						EdPy.user.saveSetting("language", newLanguage);
						i18n.setLanguage(newLanguage);
						// Reset EdWare. This refreshes the UI to use the new language, etc.
						main.reset(true);
						// Callback?
						if (callback) {
							callback(true);
						}
					} else {
						if (callback) {
							callback(false);
						}
					}
				}
			});
		} else {
			if (callback) {
				callback(false);
			}
		}
	};

	/*==================================================

	Main View Object

	==================================================*/

	var MainView = Backbone.View.extend({

		/*==================================================

		Properties

		==================================================*/

		el: [
			"<div id='mainView'></div>",
		].join(""),

		events: {

			"click #navNewProgram": "onNavNewProgram",
			"click #navSaveProgram": "onNavSaveProgram",
			"click #navSaveAsProgram": "onNavSaveAsProgram",
			"click #navToggleFullScreen": "onNavToggleFullScreen",
			"click #navAboutEdware": "onNavAboutEdware",
			"click #navSyncPrograms": "onNavSyncPrograms",
			"click #navShareProgram": "onNavShareProgram",
			"click #navFirmwareUpdates": "onNavFirmwareUpdates",
			"click #navDriveCalibration": "onNavDriveCalibration",
			"click #navSettings": "onNavSettings",
			"click #navLogin": "onNavLogin",

			"click #navCheckCode": "onNavCheckCode",
			"click #navProgramEdison": "onNavProgramEdison",

			"click #createAccount": "onCreateAccount",
			"click #resetPassword": "onResetPassword",

			"click .sub-menu a": "onMenuButton",
			"click .close-popup": "onClosePopup",
			"click .button-bar-button": "onButtonBar",

			"click .table-row-title": "onTableRow",

			"change #driveCalibrationOptionsSelect": "onSelectDriveCalibrationOption",

			"change #dontShowFirmwareUpdateWarning": "onChangeDontShowFirmwareUpdateWarning",

			"mousedown .resize-horizontal": "onMouseDownResizePalette",
			"mousedown .resize-vertical": "onMouseDownResizePalette",

		},

		views: {

		},

		dimensions: {

			app: {
				minWidth: 1280,
				minHeight: 600,
			},

			palettesColumn1: {
				width: 250,
				minWidth: 200,
				maxWidth: 400,
			},

			palettesColumn3: {
				width: 300,
				minWidth: 200,
				maxWidth: 400,
			},

			compilerOutput: {
				height: 150,
				minHeight: 150,
				maxHeight: "75",
			},

			lineHelp: {
				height: 150,
				minHeight: 150,
				maxHeight: "75",
			},

		},

		resize: {
			anchorSide: null,
			targetID: null,
			target: null,
			direction: null,
			startX: 0,
			startY: 0,
		},

		shownVersionPopup: false,

		/*==================================================

		Resize Functions

		==================================================*/

		onMouseUpResizePalette: function(e) {

			// Reset resize object.

			this.resize = {
				anchorSide: null,
				targetID: null,
				target: null,
				direction: null,
				startX: 0,
				startY: 0,
			};

			// Remove event listeners.

			$(document).unbind("mousemove");
			$(document).unbind("mouseup");

		},

		onMouseMoveResizePalette: function(e) {

			// Variables.

			var appWidth = window.innerWidth;
			var appHeight = window.innerHeight;

			var direction = "vertical";
			var key = "height";
			var keyMin = "minHeight";
			var keyMax = "maxHeight";
			var against = appHeight;

			// Direction?

			if (this.resize.direction == "horizontal") {
				direction = "horizontal";
				key = "width";
				keyMin = "minWidth";
				keyMax = "maxWidth";
				against = appWidth;
			}

			// Make sure this is a resizable element.

			if (this.dimensions.hasOwnProperty(this.resize.targetID)) {

				// Determine the new size.

				var newSize = 0;

				if (this.resize.anchorSide == "left") {
					newSize = e.pageX;
				} else if (this.resize.anchorSide == "right") {
					newSize = against - e.pageX;
				} else if (this.resize.anchorSide == "top") {
					newSize = e.pageY;
				} else if (this.resize.anchorSide == "bottom") {
					newSize = against - e.pageY;
				}

				// Determine the min and max size.

				var minSize = this.dimensions[this.resize.targetID][keyMin];
				var maxSize = this.dimensions[this.resize.targetID][keyMax];

				if (typeof minSize == "string") {
					minSize = ((minSize / 100) * against);
				}

				if (typeof maxSize == "string") {
					maxSize = ((maxSize / 100) * against);
				}

				// Make sure the new size doesn't exceed the min or max size.

				if (newSize <= minSize) {
					newSize = minSize;
				} else if (newSize >= maxSize) {
					newSize = maxSize;
				}

				// Store the new size of this element.

				this.dimensions[this.resize.targetID][key] = newSize;

			}

			// Resize UI elements.

			this.resizeElements();

		},

		onMouseDownResizePalette: function(e) {

			// Variables.

			var target = $(e.target);

			// Set resize direction and anchors.

			if (target.hasClass("resize-horizontal")) {
				this.resize.direction = "horizontal";
				if (target.hasClass("float-right")) {
					this.resize.anchorSide = "left";
				} else if (target.hasClass("float-left")) {
					this.resize.anchorSide = "right";
				}
			} else if (target.hasClass("resize-vertical")) {
				this.resize.direction = "vertical";
				if (target.hasClass("float-top")) {
					this.resize.anchorSide = "bottom";
				} else if (target.hasClass("float-bottom")) {
					this.resize.anchorSide = "top";
				}
			}

			// Valid direction?

			if (this.resize.direction) {

				// Co-ordinates.

				this.resize.startX = e.pageX;
				this.resize.startY = e.pageY;

				// Target information.

				this.resize.targetID = target.parent().prop("id");
				this.resize.target = $("#" + this.resize.targetID);

				// Add event listeners.

				$(document).mousemove(this.onMouseMoveResizePalette.bind(this));
				$(document).mouseup(this.onMouseUpResizePalette.bind(this));

			}

		},

		resizeElement: function(id) {

			// Variables.

			var appWidth = window.innerWidth;
			var appHeight = window.innerHeight;

			if (appWidth <= this.dimensions.app.minWidth) {
				appWidth = this.dimensions.app.minWidth;
			}

			if (appHeight <= this.dimensions.app.minHeight) {
				appHeight = this.dimensions.app.minHeight;
			}

			var minSize = this.dimensions[id].minHeight;
			var maxSize = this.dimensions[id].maxHeight;

			var newSize = $("#" + id).outerHeight();

			if (typeof minSize == "string") {
				minSize = ((minSize / 100) * appHeight);
			}

			if (typeof maxSize == "string") {
				maxSize = ((maxSize / 100) * appHeight);
			}

			// Make sure the new size doesn't exceed the min or max size.

			if (newSize <= minSize) {
				newSize = minSize;
			} else if (newSize >= maxSize) {
				newSize = maxSize;
			}

			// Store the new size of this element.

			this.dimensions[id].height = newSize;

		},

		resizeElements: function(e) {

			// When the window is resized -- especially when the window is made smaller -- make sure any resized elements aren't bigger than they should be.

			if (e) {
				this.resizeElement("compilerOutput");
				this.resizeElement("lineHelp");
			}

			// Variables.

			var appWidth = $(window).width();
			var appHeight = $(window).height();

			//console.log(appWidth, appHeight);

			if (appWidth <= this.dimensions.app.minWidth) {
				appWidth = this.dimensions.app.minWidth;
			}

			if (appHeight <= this.dimensions.app.minHeight) {
				appHeight = this.dimensions.app.minHeight;
			}

			var navBarHeight = $("#navBar").outerHeight();

			var palettesHeight = appHeight - navBarHeight;

			var palettesColumn1Width = this.dimensions["palettesColumn1"].width;
			var palettesColumn3Width = this.dimensions["palettesColumn3"].width;
			var palettesColumn2Width = (appWidth - (palettesColumn1Width + palettesColumn3Width));

			var compilerOutputHeight = this.dimensions.compilerOutput.height;
			var lineHelpHeight = this.dimensions.lineHelp.height;

			// Palettes wrapper.

			if ($("#palettesWrapper").length) {
				$("#palettesWrapper").css({
					width: appWidth,
					height: palettesHeight,
				});
			}

			// Palettes column 1.

			if ($("#palettesColumn1").length) {
				$("#palettesColumn1").css({
					left: 0,
					width: palettesColumn1Width,
					height: palettesHeight,
				});
			}

			// Palettes column 2.

			if ($("#palettesColumn2").length) {
				$("#palettesColumn2").css({
					left: palettesColumn1Width,
					width: palettesColumn2Width,
					height: palettesHeight,
				});
			}

			// Palettes column 3.

			if ($("#palettesColumn3").length) {
				$("#palettesColumn3").css({
					right: 0,
					width: palettesColumn3Width,
					height: palettesHeight,
				});
			}

			// Recent programs.

			if ($("#userPrograms").length) {

				var paletteHeaderHeight = $("#userPrograms").find(".title-bar").outerHeight();

				$("#userPrograms").css({
					height: palettesHeight,
				});

				$("#userPrograms #listWrapper").css({
					height: palettesHeight - paletteHeaderHeight,
				});

			}

			// Compiler output.

			if ($("#compilerOutput").length) {

				var paletteHeaderHeight = $("#compilerOutput").find(".title-bar").outerHeight();

				$("#compilerOutput").css({
					height: compilerOutputHeight,
				});

				$("#compilerOutput #contentWrapper").css({
					height: compilerOutputHeight - paletteHeaderHeight,
				});

			}

			// Code editor.

			if ($("#codeEditor").length) {

				var tabBarHeight = $("#tabBar").outerHeight();

				$("#codeEditor").css({
					height: palettesHeight - compilerOutputHeight - tabBarHeight,
				});

				this.views.codeEditor.editor.resize();

			}

			// Line help.

			if ($("#lineHelp").length) {

				var paletteHeaderHeight = $("#lineHelp").find(".title-bar").outerHeight();

				$("#lineHelp").css({
					height: lineHelpHeight,
				});

				$("#lineHelp #textWrapper").css({
					height: lineHelpHeight - paletteHeaderHeight,
				});

			}

			// Help documentation.

			if ($("#helpDocumentation").length) {

				var documentationHeight = palettesHeight - lineHelpHeight;
				var searchResultsHeight = documentationHeight - $("#searchField").outerHeight();
				var paletteHeaderHeight = $("#helpDocumentation").find(".title-bar").outerHeight();

				$("#helpDocumentation").css({
					height: documentationHeight,
				});

				$("#helpDocumentation #helpWrapper").css({
					height: documentationHeight - paletteHeaderHeight,
				});

				$("#helpDocumentation #listWrapper").css({
					height: documentationHeight - paletteHeaderHeight,
				});

				$("#helpDocumentation #listItemsWrapper").css({
					height: searchResultsHeight - paletteHeaderHeight,
				});

				$("#helpDocumentation #documentationWrapper").css({
					height: documentationHeight - paletteHeaderHeight,
				});

			}

		},

		/*==================================================

		Navigation Functions

		==================================================*/

		onNavNewProgram: function(e) {
			if (EdPy.canOpenProgram()) {
				EdPy.openUntitledProgram();
			} else {
				a("You can't open anymore programs. Close some tabs and try again.");
			}
		},

		onNavSaveProgram: function(e) {
			if (EdPy.isTutorialProgram()) {
				a(i18n.get("messages_save_program_disabled_2"));
			} else {
				if (EdPy.isUntitledProgram() || EdPy.isSharedProgram()) {
					$("#navSaveAsProgram").trigger("click");
				} else {
					this.save();
				}
			}
		},

		onNavSaveAsProgram: function(e) {
			// That.
			var that = this;
			// Can they save?
			if (EdPy.hasHitTabLimit()) {
				a("Uh oh! You need to close some tabs before trying to save as a new program.");
			} else {
				if (EdPy.isTutorialProgram()) {
					a(i18n.get("messages_save_program_disabled_2"));
				} else {
					UI.newPopup({
						type: "save_as",
						title: i18n.get("popup_save_as_title"),
						onButton: function(i, parent) {
							var validated = Utils.validateProgramName($("#saveAsProgramName").val());
							if (validated.error) {
								a(validated.message);
							} else if (validated.success) {
								// Set temporary properties for this program.
								EdPy.setTemporaryProgramName(validated.programName);
								// Save program.
								var saveProgram = function() {
									that.save(parent);
								};
								// Check if the program exists first.
								EdPy.getProgramByName(EdPy.getTemporaryProgramName(), function(exists) {
									if (exists) {
										UI.newConfirm({
											autoDestroy: true,
											message: i18n.get("messages_save_program_confirm_overwrite"),
											onButton: function(i, parent) {
												if (i === 1) {
													saveProgram();
												}
											}
										});
									} else {
										saveProgram();
									}
								});
							}
						}
					});
				}
			}
		},

		onNavSyncPrograms: function(e) {
			// That.
			var that = this;
			// Confirm sync.
			UI.newConfirm({
				autoDestroy: true,
				message: i18n.get("popup_sync_local_programs_confirmation"),
				onButton: function(i) {
					if (i === 1) {
						// Show a progress pop-up.
						var syncProgress = UI.newProgress({
							message: i18n.get("popup_sync_local_programs_pending"),
						});
						// Variables.
						var id = 0;
						var programs = EdPy.getLocalPrograms();
						// Sync wrapper.
						var syncProgram = function(id) {
							if (programs[id]) {
								// Current program.
								var program = programs[id];
								// Check if the program exists by name.
								EdPy.getProgramByName(program.program_name, function(exists) {
									// If the program exists, modify the name so we don't overwrite the program stored in the database.
									if (exists) {
										program.program_name += ("_" + Date.now());
									}
									// Sync the program.
									EdPy.syncProgram(program, function(success, message) {
										id++;
										syncProgram(id);
									});
								});
							} else {
								// Remove progress pop-up.
								syncProgress.trigger("destroyMe");
								// Delete local programs.
								EdPy.setLocalPrograms([]);
								// Change event.
								EdPy.trigger("programs.change");
								// Let the user know the operation has finished.
								a(i18n.get("popup_sync_local_programs_success"));
							}
						};
						// Sync first program.
						syncProgram(id);
					}
				}
			});
		},

		onNavToggleFullScreen: function(e) {
			if (!document.fullscreenElement && !document.mozFullScreenElement && !document.webkitFullscreenElement && !document.msFullscreenElement) {
				if (document.documentElement.requestFullscreen) {
					document.documentElement.requestFullscreen();
				} else if (document.documentElement.msRequestFullscreen) {
					document.documentElement.msRequestFullscreen();
				} else if (document.documentElement.mozRequestFullScreen) {
					document.documentElement.mozRequestFullScreen();
				} else if (document.documentElement.webkitRequestFullscreen) {
					document.documentElement.webkitRequestFullscreen(document.documentElement.ALLOW_KEYBOARD_INPUT);
				}
			} else {
				if (document.exitFullscreen) {
					document.exitFullscreen();
				} else if (document.msExitFullscreen) {
					document.msExitFullscreen();
				} else if (document.mozCancelFullScreen) {
					document.mozCancelFullScreen();
				} else if (document.webkitExitFullscreen) {
					document.webkitExitFullscreen();
				}
			}
		},

		onNavAboutEdware: function(e) {
			UI.newPopup({
				type: "about_edware",
				title: i18n.get("popup_about_edware_title"),
			});
		},

		onNavFirmwareUpdates: function() {
			window.open("https://meetedison.com/edison-robot-support/firmware-update/");
			/*UI.newPopup({
				type: "firmware_updates",
				title: i18n.get("popup_firmware_updates_title"),
				onButton: function(i, parent) {
					if (i === 0) {
						var width = 400;
						var height = 200;
						var left = ($(window).width() - width) / 2;
						var top = ($(window).height() - height) / 2;
						var opts = 'width=' + width + ',height=' + height + ',top=' + top + ',left=' + left + ',scrollbars=yes';
						window.open("http://www.edwareapp.com/firmware_updates/firmware-latest.wav", "Firmware Update", opts);
						return false;
					}
				}
			});*/
		},

		onSelectDriveCalibrationOption: function(e) {
			if (!Values.isIE) {
				var selected = $(e.target).find("option:selected").val();
				$("#driveCalibrationPlayerWrapper").html("<hr><audio controls='controls' preload='auto' disabled><source src='http://www.edwareapp.com/firmware_updates/" + selected + "' type='audio/wav'></audio>");
			}
		},

		onNavDriveCalibration: function() {
			window.open("https://meetedison.com/edison-robot-support/drive-calibration/");
			/*UI.newPopup({
				type: "drive_calibration",
				title: i18n.get("popup_firmware_updates_title"),
				onButton: function(i, parent) {
					if (i === 0) {
						var selected = $("#driveCalibrationOptionsSelect").find("option:selected").val();
						if (selected === 0) {
							return a(i18n.get("messages_drive_calibration_select_option"));
						}
						var width = 400;
						var height = 200;
						var left = ($(window).width() - width) / 2;
						var top = ($(window).height() - height) / 2;
						var opts = 'width=' + width + ',height=' + height + ',top=' + top + ',left=' + left + ',scrollbars=yes';
						window.open("http://www.edwareapp.com/firmware_updates/" + selected, "Firmware Update", opts);
						return false;
					}
				},
			});*/
		},

		onChangeDontShowFirmwareUpdateWarning: function() {
			EdPy.user.saveSettings({
				"dontShowFirmwareUpdateWarning": $("#dontShowFirmwareUpdateWarning").is(":checked"),
			});
			console.log(EdPy.user);
			//console.log(1);
			//console.log(this);
			//console.log($("#dontShowFirmwareUpdateWarning").is(":checked"));
		},

		onNavSettings: function() {
			// That.
			var that = this;
			// Load pop-up function.
			UI.newPopup({
				type: "settings",
				title: i18n.get("popup_settings_title"),
				validation: {
					rules: {
						emailAddress: {
							required: true,
							email: false,
							isEmail: true,
						},
					},
					messages: {
						emailAddress: {
							required: i18n.get("messages_account_validation_invalid_email"),
							isEmail: i18n.get("messages_account_validation_invalid_email"),
						},
					},
				},
				onButton: function(i, parent) {
					if (i === 1) {
						var saveLocal = function() {
							// Hide the settings popup.
							parent.trigger("destroyMe");
							// Ask the user if they want to change their language.
							/*updateLanguage(that, $("#selectLanguage").val(), "messages_language_change_from_settings", function(changed) {
							a(i18n.get("messages_settings_save_success"));
						});*/
					};
					if (EdPy.user.isLoggedIn()) {
						if ($("#popupForm").valid()) {
							EdPy.user.updateAccount({
								email: $("#emailAddress").val(),
								language: "en",
								//language: $("#selectLanguage").val(),
							}, function(success, message, message_i18n_key) {
								if (success) {
									// Refresh sub-menu so it shows the correct email address (if they changed it).
									that.views.navBar.refresh();
									// Save local settings.
									saveLocal();
								} else {
									a(i18n.get(message_i18n_key));
								}
							});
						}
					} else {
						saveLocal();
					}
				} else {
					parent.trigger("destroyMe");
				}
			},
		});
	},

	onNavShareProgram: function(e) {
		if (EdPy.isTutorialProgram()) {
			a(i18n.get("messages_share_program_disabled_1"));
		} else {
			if (EdPy.isUntitledProgram()) {
				a(i18n.get("messages_share_program_disabled_2"));
			} else {
				UI.newPopup({
					type: "share_program",
					title: i18n.get("popup_share_program_title"),
					socialShare: true,
				});
			}
		}
	},

	onNavLogin: function() {
		// That.
		var that = this;
		// Login or logout?
		if (EdPy.user.isLoggedIn()) {
			EdPy.user.logout(function(loggedout) {
				if (loggedout) {
					that.reset(true);
					a(i18n.get("messages_logout_success"));
				} else {
					a(i18n.get("messages_logout_error"));
				}
			});
		} else {
			UI.newPopup({
				type: "login",
				title: i18n.get("popup_login_title"),
				onButton: function(i, parent) {
					if (i === 0) {
						var email = $("#email").val().trim();
						var password = $("#password").val().trim();
						if (email === "" || password === "") {
							a(i18n.get("messages_account_validation_invalid_credentials"));
						} else {
							if (Utils.validEmailAddress(email)) {
								EdPy.user.login({
									email: email,
									password: password,
								}, function(success, message, message_i18n_key, settings) {
									if (success) {
										// Hide login popup.
										parent.trigger("destroyMe");
										// Reset app.
										that.reset();
										// Login success!
										a(i18n.get("messages_login_success"));
										/*// Hide login pop-up.
										parent.trigger("destroyMe");
										// Ask the user if they want to change their language.
										updateLanguage(that, settings.language, "messages_language_change_from_sync", function(changed) {
										// Reset EdWare?
										if (!changed) {
										that.reset();
									}
									// Login success!
									a(i18n.get("messages_login_success"));
								});*/
							} else {
								a(i18n.get(message_i18n_key));
							}
						});
					} else {
						a(i18n.get("messages_account_validation_invalid_credentials"));
					}
				}
			}
		},
	});
}
},

onNavCheckCode: function(e) {
	EdPy.api.action("check_code", EdPy.getProgramText(), function(response, status, xhr) {
		EdPy.setCompilerOutput(response);
	});
},

onNavProgramEdison: function(e) {

	// Create pop-up.

	var programEdison = UI.newPopup({
		type: "compile_program",
		title: i18n.get("popup_program_edison_title"),
		onButton: function(i, parent) {
			if (i === 0) {
				if (Values.isIE) {
					var width	=	400;
					var height	=	200;
					var left	=	($(window).width() - width) / 2;
					var top		=	($(window).height() - height) / 2;
					var opts	=	'width=' + width + ',height=' + height + ',top=' + top + ',left=' + left + ',scrollbars=yes';
					window.open(Values.programURL, "Play WAV", opts);
					return false;
				} else {
					document.getElementById("programEdisonPlayer").play();
				}
			}
		},
	});

	// Start downloading the WAV file.

	EdPy.api.action("compile_program", EdPy.getProgramText(), function(response, status, xhr) {

		// Remove the loading spinner.

		$("#creatingWavMessage").remove();

		// Pop-up references.

		var $popupInner = $("#programEdison");
		var $popupOuter = $popupInner.parent();

		// Success or failure?

		if (response.code == 200) {
			// Store the URL for later use.
			Values.programURL = response.compiler.wav_url;
			// Create 'Program Edison' button.
			$popupOuter.append(UI.newButtonBar([
				[i18n.get("popup_program_edison_button_program")],
			]));
			// Update audio player?
			if (!Values.isIE) {
				$("#programEdisonPlayer").html("<source src='{0}' type='audio/wav'>".format(Values.programURL));
			}
		} else {
			// Destroy the popup.
			programEdison.trigger("destroyMe");
			// Update the output window with the errors.
			EdPy.setCompilerOutput(response);
		}

	});

},

onCreateAccount: function(e) {

	// That.

	var that = this;

	// Create pop-up.

	UI.newPopup({
		type: "create_account",
		title: i18n.get("popup_create_account_title"),
		validation: {
			rules: {
				regEmail: {
					required: true,
					email: false,
					isEmail: true,
				},
				regPassword1: {
					required: true,
					minlength: 5,
				},
				regPassword2: {
					required: true,
					minlength: 5,
					equalTo: "#regPassword1",
				},
			},
			messages: {
				regEmail: {
					required: i18n.get("messages_account_validation_invalid_email"),
					isEmail: i18n.get("messages_account_validation_invalid_email"),
				},
				regPassword1: {
					required: i18n.get("messages_account_validation_invalid_password"),
					minlength: $.validator.format(i18n.get("messages_account_validation_invalid_password_length")),
				},
				regPassword2: {
					required: i18n.get("messages_account_validation_confirm_password"),
					minlength: $.validator.format(i18n.get("messages_account_validation_invalid_password_length")),
					equalTo: i18n.get("messages_account_validation_password_mismatch"),
				}
			},
		},
		onButton: function(i, parent) {
			if ($("#popupForm").valid()) {
				EdPy.user.createAccount({
					email: $("#regEmail").val(),
					password: $("#regPassword1").val(),
					language: i18n.getLanguage(),
				}, function(registered, loggedin, message, message_i18n_key) {
					if (registered) {
						// Close popup.
						$(".popup-wrapper").trigger("destroyMe");
						// Message?
						if (registered && loggedin) {
							//that.views.navBar.refresh();
							a(i18n.get(message_i18n_key));
						} else {
							a(i18n.get(message_i18n_key));
						}
						// Reset view.
						that.reset();
						//that.reset(true);
					} else {
						a(i18n.get(message_i18n_key));
					}
				});
			}
		},
	});

},

onResetPassword: function(e) {

	// That.

	var that = this;

	// Create pop-up.

	UI.newPopup({
		type: "reset_password",
		title: i18n.get("popup_reset_password_title"),
		onButton: function(i, parent) {
			var emailAddress = $("#forgotEmailAddress").val().trim();
			if (emailAddress === "") {
				a(i18n.get("messages_account_validation_invalid_email"));
			} else {
				if (Utils.validEmailAddress(emailAddress)) {
					EdPy.user.resetPassword(emailAddress, function(success, message, message_i18n_key) {
						a(i18n.get(message_i18n_key));
						if (success) {
							parent.trigger("destroyMe");
						}
					});
				} else {
					a(i18n.get("messages_account_validation_invalid_email"));
				}
			}
		},
	});

},

onViewDocumentation: function(e) {
	var $target = $(e.target);
	if ($target.hasClass("btn-view-documentation")) {
		this.views.codeEditor.editor.focus();
		this.views.helpDocumentation.openItem($target.data().title);
	}
},

onMenuButton: function(e) {
	this.views.navBar.hideMenu();
},

onClosePopup: function(e) {

	var $me = $(e.target);
	var $popup = $me.closest(".popup-wrapper");

	var params = $popup.data("params");

	if (params.onClose) {
		if (params.autoDestroy) {
			$popup.trigger("destroyMe");
		}
		params.onClose($popup);
	} else {
		$popup.trigger("destroyMe");
	}

},

onButtonBar: function(e) {

	var $me = $(e.target);
	var $popup = $me.closest(".popup-wrapper");

	var index = $me.data("i");
	var params = $popup.data("params");

	if (params.onButton) {
		if (params.autoDestroy) {
			$popup.trigger("destroyMe");
		}
		params.onButton(index, $popup);
	} else {
		$popup.trigger("destroyMe");
	}

},

onTableRow: function(e) {

	var $me = $(e.target);
	var $popup = $me.closest(".popup-wrapper");

	var index = $me.data("i");
	var params = $popup.data("params");

	if (params.table && params.table.onRow) {
		if (params.autoDestroy) {
			$popup.trigger("destroyMe");
		}
		params.table.onRow(index, params.table.data[index], $popup, params.table.category);
	} else {
		$popup.trigger("destroyMe");
	}

},

/*==================================================

Lifecycle Functions

==================================================*/

delete: function(id, type) {
	var confirmDelete;
	confirmDelete = UI.newConfirm({
		autoDestroy: false,
		message: "Are you sure you want to delete this program? This action cannot be undone.",
		onButton: function(i) {
			if (i === 1) {
				EdPy.deleteProgram(id, type, function(success) {
					// Close popup.
					confirmDelete.trigger("destroyMe");
					// Error?
					if (!success) {
						a("An unknown error occurred while deleting this program.");
					}
				});
			} else {
				// Close popup.
				confirmDelete.trigger("destroyMe");
			}
		}
	});
},

load: function(id, type) {
	if (EdPy.canOpenProgram()) {
		if (EdPy.isProgramOpen(id)) {
			EdPy.openOpenProgram(id);
		} else {
			EdPy.openProgram(id, type);
		}
	} else {
		a("You can't open anymore programs. Close some and then try again.");
	}
},

save: function(popup) {
	EdPy.saveProgram(function(success, message) {
		// Is there a pop-up to close?
		if (popup) {
			popup.trigger("destroyMe");
		}
		// Success or error?
		if (success) {
			// No longer dirty.
			EdPy.isDirty(false);
			// Success!
			a(i18n.get("messages_save_program_success"));
		} else {
			// Is there a message to display?
			if (message) {
				a(message);
			}
		}
		// Clear temporary program name.
		EdPy.setTemporaryProgramName("");
	});
},

onClosePage: function() {
	if (EdPy.isAnyProgramDirty()) {
		return i18n.get("messages_confirm_exit_app");
	}
},

remove: function() {
	//$(window).off("resize", this.resizeElements);
	//$(window).off("beforeunload", this.onClosePage);
	$(window).unbind("resize");
	$(window).unbind("beforeunload");
	Backbone.View.prototype.remove.apply(this, arguments);
},

reset: function(resetProgramData) {

	// Reset program data.

	if (resetProgramData) {
		EdPy.resetProgramData();
	}

	// Render the app.

	this.render();

	// Refresh the navbar.

	this.views.navBar.refresh();

},

render: function() {

	// That.

	var that = this;

	// This view's HTML.

	this.$el.html([
		"<noscript><iframe src='https://www.googletagmanager.com/ns.html?id=GTM-T8KHBR9' height='0' width='0' style='display:none; visibility:hidden'></iframe></noscript>",
		"<div id='navBarWrapper'></div>",
		"<div id='palettesWrapper'>",
		"<div id='palettesColumn1'><div class='resize-horizontal float-right'></div></div>",
		"<div id='palettesColumn2'></div>",
		"<div id='palettesColumn3'><div class='resize-horizontal float-left'></div></div>",
		"</div>",
	].join(""));

	// Create navigation bar.

	this.views.navBar = new NavigationBar();

	this.$("#navBarWrapper").append(this.views.navBar.el);

	// Create programs list.

	this.views.userPrograms = new UserPrograms();

	this.$("#palettesColumn1").append(this.views.userPrograms.el);

	this.views.userPrograms.on("event", function(e) {
		if (e.type == "loadProgram") {
			that.load(e.programID, e.programType);
		} else if (e.type == "deleteProgram") {
			that.delete(e.programID, e.programType);
		}
	});

	// Create compiler output.

	this.views.compilerOutput = new CompilerOutput();

	this.$("#palettesColumn2").append(this.views.compilerOutput.el);

	// Create help documentation.

	this.views.helpDocumentation = new HelpDocumentation();

	this.$("#palettesColumn3").append(this.views.helpDocumentation.el);

	this.views.helpDocumentation.on("event", function(e) {
		if (e.type == "closeDocumentation" || e.type == "openDocumentation") {
			that.resizeElements();
		}
	});

	// Create line help.

	this.views.lineHelp = new LineHelp();

	this.$("#palettesColumn3").append(this.views.lineHelp.el);

	// Create code editor.

	this.views.codeEditor = new CodeEditor();

	this.$("#palettesColumn2").append(this.views.codeEditor.el);

	this.views.codeEditor.on("event", function(e) {
		if (e.type == "change") {
			that.views.lineHelp.parseEvent(e);
		} else if (e.type == "changeText") {
			that.views.lineHelp.setOptions(e);
		} else if (e.type == "changeSelection") {
			that.views.lineHelp.parseEvent(e);
		} else if (e.type == "snippetsRegistered") {
			that.views.lineHelp.snippets = that.views.codeEditor.snippets;
		}
	});

	this.views.codeEditor.init();

	// Resize the UI elements.

	this.resizeElements();

	// Show 'Edison Version' popup.

	if (!this.shownVersionPopup) {

		this.shownVersionPopup = true;

		UI.newPopup({
			type: "edison_version",
			wide: true,
			title: "Edison Version",
			onButton: function(i, parent) {
				// Set version number.
				EdPy.setVersion(i+1);
				// Get version text.
				let versionText = EdPy.getProgramText(true);
				// Update program text.
				EdPy.setProgramText(versionText);
				that.views.codeEditor.editor.setValue(versionText, 1);
				that.views.codeEditor.editor.focus();
				// Hide popup.
				parent.trigger("destroyMe");
				// Show firmware update warning popup?
				if (i == 0) {
					if (!EdPy.user.settings.dontShowFirmwareUpdateWarning) {
						UI.newPopup({
							type: "firmware_warning",
							wide: true,
							title: "Firmware Update Warning",
							onButton: function(i, parent) {
								parent.trigger("destroyMe");
							}
						});
					}
				}
			}
		});

	}

},

initialize: function() {


	// Event listeners.

	$(window).on("resize", this.resizeElements.bind(this));
	$(window).on("beforeunload", this.onClosePage);
	$("body").on("click", this.onViewDocumentation.bind(this));

},

});

/*==================================================

Return Main View

==================================================*/

return MainView;

});

define('app/libs/extensions',[

], function() {

	"use strict";

	return {

		init: function() {

			// Math

			if (typeof Math.roundTo === "undefined") {
				Math.roundTo = function(num, step) {
					return Math.floor((num / step) + .5) * step;
				}
			}

			// Date

			if (typeof Date.prototype.customFormat != "function") {
				Date.prototype.customFormat = function(formatString) {
					var YYYY,YY,MMMM,MMM,MM,M,DDDD,DDD,DD,D,hhh,hh,h,mm,m,ss,s,ampm,AMPM,dMod,th;
					var dateObject = this;
					YY = ((YYYY=dateObject.getFullYear())+"").slice(-2);
					MM = (M=dateObject.getMonth()+1)<10?('0'+M):M;
					MMM = (MMMM=["January","February","March","April","May","June","July","August","September","October","November","December"][M-1]).substring(0,3);
					DD = (D=dateObject.getDate())<10?('0'+D):D;
					DDD = (DDDD=["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"][dateObject.getDay()]).substring(0,3);
					th=(D>=10&&D<=20)?'th':((dMod=D%10)==1)?'st':(dMod==2)?'nd':(dMod==3)?'rd':'th';
					formatString = formatString.replace("#YYYY#",YYYY).replace("#YY#",YY).replace("#MMMM#",MMMM).replace("#MMM#",MMM).replace("#MM#",MM).replace("#M#",M).replace("#DDDD#",DDDD).replace("#DDD#",DDD).replace("#DD#",DD).replace("#D#",D).replace("#th#",th);
					h=(hhh=dateObject.getHours());
					if (h==0) h=24;
					if (h>12) h-=12;
					hh = h<10?('0'+h):h;
					AMPM=(ampm=hhh<12?'am':'pm').toUpperCase();
					mm=(m=dateObject.getMinutes())<10?('0'+m):m;
					ss=(s=dateObject.getSeconds())<10?('0'+s):s;
					return formatString.replace("#hhh#",hhh).replace("#hh#",hh).replace("#h#",h).replace("#mm#",mm).replace("#m#",m).replace("#ss#",ss).replace("#s#",s).replace("#ampm#",ampm).replace("#AMPM#",AMPM);
				};
			}

			// String

			if (typeof String.prototype.first != "function") {
				String.prototype.first = function(amount) {
					return this.slice(0, amount);
				};
			}

			if (typeof String.prototype.isSpace != "function") {
				String.prototype.isSpace = function() {
					return this.trim() === "";
				};
			}

			if (typeof String.prototype.startsWith != "function") {
				String.prototype.startsWith = function(str) {
					return this.slice(0, str.length) == str;
				};
			}

			if (typeof String.prototype.endsWith != "function") {
				String.prototype.endsWith = function(str) {
					return this.slice(-str.length) == str;
				};
			}

			if (typeof String.prototype.format != "function") {
				String.prototype.format = function() {
					var args = arguments;
					return this.replace(/{(\d+)}/g, function(match, number) {
						return typeof args[number] != 'undefined' ? args[number] : match;
					});
				};
			}

			if (typeof String.isDigit != "function") {
				String.isDigit = function(s) {
					if (s.trim().length === 0) {
						return false;
					}
					return !isNaN(s);
				};
			}

			// Array

			if (typeof Array.max != "function") {
				Array.max = function(array) {
					return Math.max.apply(Math, array);
				};
			}

			if (typeof Array.min != "function") {
				Array.min = function(array) {
					return Math.min.apply(Math, array);
				};
			}

			if (typeof Array.move != "function") {
				Array.move = function (array, old_index, new_index) {
					while (old_index < 0) {
						old_index += array.length;
					}
					while (new_index < 0) {
						new_index += array.length;
					}
					if (new_index >= array.length) {
						var k = new_index - array.length;
						while ((k--) + 1) {
							array.push(undefined);
						}
					}
					array.splice(new_index, 0, array.splice(old_index, 1)[0]);
					return array;
				};
			}

			// Object

			if (typeof Object.isEmpty != "function") {
				Object.isEmpty = function(obj) {
					return Object.keys(obj).length === 0;
				};
			}

			if (typeof Object.renameKey != "function") {
				Object.renameKey = function (obj, oldName, newName) {
					// Do nothing if the names are the same.
					if (oldName == newName) {
						return obj;
					}
					// Check for the old property name to avoid a ReferenceError in strict mode.
					if (obj.hasOwnProperty(oldName)) {
						obj[newName] = obj[oldName];
						delete obj[oldName];
					}
					return obj;
				};
			}

		},

	};

});
var SpriteSpinner = function(el, options) {

  var self = this,
  img = el.children[0];

  this.interval = options.interval || 10;
  this.diameter = options.diameter || img.width;
  this.count = 0;
  this.el = el;

  img.setAttribute("style", "position:absolute");

  el.style.width = this.diameter+"px";
  el.style.height = this.diameter+"px";

  return this;

};

SpriteSpinner.prototype.start = function() {
  var self = this,
  count = 0,
  img = this.el.children[0];
  this.el.display = "block";
  self.loop = setInterval(function(){
    if (count == 19) {
      count = 0;
    }
    img.style.top = (-self.diameter*count)+"px";
    count++;
  }, this.interval);
};

SpriteSpinner.prototype.stop = function() {
  clearInterval(this.loop);
  this.el.style.display = "none";
};
define("spritespinner", (function (global) {
    return function () {
        var ret, fn;
        return ret || global.SpriteSpinner;
    };
}(this)));

define('app/app',[

	"app/views/main",
	"edpy",
	"values",
	"utils",
	"app/libs/extensions",
	"app/libs/i18n",
	"selectpicker",
	"spritespinner",

], function(MainView, EdPy, Values, Utils, Extensions, i18n, selectpicker, SpriteSpinner) {

	"use strict";

	return {

		initialize: function() {

			// Variables.

			var $selectLanguage = $("#selectLanguage");
			var $dontShowAgain = $("#dontShowAgain");
			var $sharedProgramID = $("#sharedProgramID");
			var $launchApp = $("#launchApp");
			var $launchingApp = $("#launchingApp");

			var sharedProgramID = $sharedProgramID.val();

			// Functions.

			var launchApp = function() {

				// Save local settings.

            console.log("debug. launchApp()");

				EdPy.user.saveSettings({
					"language": $selectLanguage.val(),
					"autoLaunch": $dontShowAgain.is(":checked"),
				});

				// Init extensions module.

				Extensions.init();

				// Init i18n module.

				i18n.init();

				// Load shared program or new program?

				var loadMainView = function() {

					// Instantiate main view.

					var mainView = new MainView();

					// If this is a shared program don't show the version popup.

					if (sharedProgramID) {
						mainView.shownVersionPopup = true;
					}

					// Set body HTML.

					$("body").html(mainView.el);

					// Reset the main view.

					mainView.reset(true);

				};

				if (sharedProgramID) {

					EdPy.getSharedProgramByID(sharedProgramID, function() {
						loadMainView();
					});

				} else {

					loadMainView();

				}

			};

			var getSettings = function() {

				EdPy.user.getSettings(function(settings) {


               console.log("debug. getSettings(): " + JSON.stringify(settings));

					// See if their preferred language is in the dropdown. If it's not, it means there was a problem
					// getting the languages from the server.

					/*if ($selectLanguage.find("option[value=" + settings.language + "]").length) {
						$selectLanguage.val(settings.language).selectpicker("refresh");
					} else {
						$selectLanguage.val("en").selectpicker("refresh");
					}*/

					$selectLanguage.val("en");

					// Enable the 'Launch App' button.

					$launchApp.prop("disabled", false);

					// Load shared program or auto launch the app?

					$dontShowAgain.prop("checked", settings.autoLaunch);

					if (sharedProgramID || settings.autoLaunch) {
						$launchApp.click();
					}

				});

			};

			var initUser = function() {
				EdPy.user.init(function(e) {
					if (e.type == "session") {
						if (EdPy.user.isLoggedIn()) {
							EdPy.user.validateToken(function(valid) {
								getSettings();
							});
						} else {
							getSettings();
						}
					}
				});
			};

			// Init the loading spinner.

			$(".sprite-spinner").each(function(i) {
				new SpriteSpinner(this, {interval: 50}).start();
			});

			// Click handler for 'Launch EdWare' button.

			$launchApp.click(function() {

				// Variables.

				var selectedLanguage = $selectLanguage.val();

				// Only launch if they've selected a valid language.

				if (selectedLanguage) {

					// Show the loading spinner.

					$launchingApp.removeClass("hide");

					// If the user is logged in save the language to their account.

					if (EdPy.user.isLoggedIn()) {
						EdPy.user.updateAccount({
							language: selectedLanguage,
						}, function(success, message, message_i18n_key) {
							launchApp();
						});
					} else {
						launchApp();
					}

				}

			});

			// Get the language options.

			i18n.getLanguages(function() {
				initUser();
			});

		},

	};

});

