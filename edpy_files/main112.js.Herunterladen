		},

	];

	/*==================================================
	Python Functions
	==================================================*/

	var pythonFunctions = [

		{
			name: "abs()",
			description: "Return the absolute value of the argument.",
			fullHelp: true,
		},

		{
			name: "len()",
			description: "Return the number of items of a list or tune string.",
			fullHelp: true,
		},

		{
			name: "ord()",
			description: "Return the integer value of a character from a tune string.",
			fullHelp: true,
		},

		{
			name: "chr()",
			description: "Return the ascii character associated with an integer for use in tune strings.",
			fullHelp: true,
		},

		{
			name: "range()",
			description: "Return a list of integers from 0 to limit.",
			fullHelp: true,
		},

	];

	/*==================================================
	Create Autocomplete Data
	==================================================*/

	var autocompleteSnippets = [];

	for (var i in classMembers) {

		// Class member data.

		var item = classMembers[i];

		// Variables.

		var tabTrigger = "";
		var content = "";
		var title = "";

		// Is the class member a function or constant or variable?

		if (item.isFunction) {
			tabTrigger = className  + "." + item.name + "()";
			content = className  + "." + item.name + "(";
			title = className  + "." + item.name + "(";
			if (item.parameters) {
				content += item.parameters;
				title += item.parametersDisplay;
			}
			content += ")";
			title += ")";
		} else {
			tabTrigger = className + "." + item.name;
			content = tabTrigger;
			title = tabTrigger;
		}

		// Add to autocomplete list.

		autocompleteSnippets.push({
			tabTrigger: tabTrigger,
			content: content,
			title: title,
			help: item.description ? item.description : null,
			fullHelp: item.fullHelp ? item.fullHelp : null,
		});

	}

	for (var i in pythonFunctions) {
		var item = pythonFunctions[i];
		autocompleteSnippets.push({
			tabTrigger: item.name ? item.name : null,
			content: item.name ? item.name : null,
			help: item.description ? item.description : null,
			fullHelp: item.fullHelp ? item.fullHelp : null,
		});
	}

	/*==================================================
	Ed Class Line Help
	==================================================*/

	var edClassLineHelp = {

		// Constants

		constants: {

			"Ed.FORWARD": "drives forward for",
