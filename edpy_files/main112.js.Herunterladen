			"save_as": function(params) {

				// Create local programs warning?

				var warning = "";

				if (!EdPy.user.isLoggedIn()) {
					warning = "<div class='alert alert-warning fifteen-margin-top no-margin-bottom text-center' role='alert'>{0}</div>".format(i18n.get("popup_save_as_warning"));
				}

				// Return pop-up html.

				return [
