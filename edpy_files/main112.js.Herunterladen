		objs: {
			bricList: "#bricList",
			propBox: "#bricProperties",
			navBar: "#navBar",
			//workingAreaWrapper: "#workingAreaWrapper",
		},

		showBodyOverflow: showBodyOverflow,

		hideBodyOverflow: hideBodyOverflow,

		newButtonBar: newButtonBar,

		contentFunctions: {

			"firmware_warning": function() {
				return [
					"<div id='firmwareWarning' class='popup-contents firmware-warning'>",
						"<p>Programming Edison V1 with EdPy requires the June 2017 firmware update.</p>",
