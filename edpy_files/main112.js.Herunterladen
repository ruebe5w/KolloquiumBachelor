	name: "Clap_controlled_driving",
	text: tut1,
	text_saved: tut1,
	dirty: false,
	tutorial: true,
	sharedWithUs: false
}));

var tut2 = [
	"",
	"#-------------Setup----------------",
	"",
	"import Ed",
	"",
	"Ed.EdisonVersion = Ed.V2",
	"",
	"Ed.DistanceUnits = Ed.CM",
	"Ed.Tempo = Ed.TEMPO_MEDIUM",
	"",
	"#--------Your code below-----------",
	"",
	"#turn on obstacle detection",
	"Ed.ObstacleDetectionBeam(Ed.ON)",
	"#set up veriables",
	"SideTurn = 0",
	"obstacle = 0",
	"",
	"#loop forever",
	"while True:",
	"\t#start Edison driving, without a duration, so Edison is moving while checking for obstacles",
	"\tEd.Drive(Ed.FORWARD, Ed.SPEED_5, Ed.DISTANCE_UNLIMITED)",
	"\t#read obstacle detection state into a variable",
	"\tobstacle = Ed.ReadObstacleDetection()",
	"\t#check if any obstacle has been detected",
	"\tif obstacle>Ed.OBSTACLE_NONE:",
	"\t\t#turn on both LEDs and back up",
	"\t\tEd.LeftLed(Ed.ON)",
	"\t\tEd.RightLed(Ed.ON)",
	"\t\tEd.PlayBeep()",
	"\t\tEd.Drive(Ed.BACKWARD, Ed.SPEED_5, 3)",
	"\t\t#look at where the obstacle is and turn away from it (if obstacle ahead turn a random direction)",
	"\t\tif obstacle==Ed.OBSTACLE_LEFT:",
	"\t\t\tEd.Drive(Ed.SPIN_RIGHT, Ed.SPEED_5, 90)",
	"\t\telif obstacle==Ed.OBSTACLE_RIGHT:",
	"\t\t\tEd.Drive(Ed.SPIN_LEFT, Ed.SPEED_5, 90)",
	"\t\telif obstacle==Ed.OBSTACLE_AHEAD:",
	"\t\t\tif SideTurn ==1:",
	"\t\t\t\tEd.Drive(Ed.SPIN_RIGHT, Ed.SPEED_5, 90)",
	"\t\t\telse:",
	"\t\t\t\tEd.Drive(Ed.SPIN_LEFT, Ed.SPEED_5, 90)",
	"\t#after the obstacle has been avoided turn the LEDs off",
	"\tEd.LeftLed(Ed.OFF)",
	"\tEd.RightLed(Ed.OFF)",
	"\t#change the direction varible, when no obstacle is being detected, this value changes rapidly, giving a sudo random direction when used",
	"\tif SideTurn==1:",
	"\t\tSideTurn=0",
	"\telse:",
	"\t\tSideTurn=1",
	"\t",
	"\t",
].join("\n");

